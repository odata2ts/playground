{
  "_version": "1.12.0",

  "sap.app": {
    "id": "org.odata2ts.tst",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "dataSources": {
      "trippin": {
        "uri": "https://services.odata.org/TripPinRESTierService/(S(uw0lp31en3kfizuwvgtataed))/",
        "type": "OData",
        "settings": {
          "odataVersion": "4.0"
        }
      }
    }
  },

  "sap.ui": {
    "technology": "UI5",
    "icons": {},
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },

  "sap.ui5": {
    "rootView": {
      "viewName": "org.odata2ts.tst.view.App",
      "type": "XML",
      "async": true,
      "id": "app"
    },

    "dependencies": {
      "minUI5Version": "1.111.0",
      "libs": {
        "sap.ui.core": {},
        "sap.ui.layout": {},
        "sap.ui.unified": {},
        "sap.m": {}
      }
    },

    "handleValidation": true,

    "contentDensities": {
      "compact": true,
      "cozy": true
    },

    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "org.odata2ts.tst.i18n.i18n"
        }
      },
      "trippin": {
        "dataSource": "trippin"
      }
    },

    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "org.odata2ts.tst",
        "viewName": "app",
        "controlId": "app",
        "controlAggregation": "pages",
        "async": true
      },
      "routes": [
        {
          "pattern": "",
          "name": "main",
          "target": "main"
        },
        {
          "pattern": "mobx",
          "name": "mobx",
          "target": "mobx"
        }
      ],
      "targets": {
        "main": {
          "viewId": "main",
          "viewName": "classic/view/Main"
        },
        "mobx": {
          "viewId": "mobx",
          "viewName": "mobx/view/Main"
        }
      }
    }
  }
}
