{
  "name": "tst",
  "version": "1.0.0",
  "description": "UI5 Application: Mobx integration",
  "author": "texttechne",
  "license": "MIT",
  "scripts": {
    "build": "odata2ts && ui5 build --clean-dest",
    "build:opt": "odata2ts && ui5 build self-contained --clean-dest --all",
    "start": "ui5 serve --port 8080 -o index.html",
    "start:dist": "ui5 serve  --port 8080 -o index.html --config ui5-dist.yaml",
    "ts-typecheck": "tsc --noEmit",
    "gen-odata": "odata2ts",
    "lint": "eslint webapp"
  },
  "devDependencies": {
    "@odata2ts/odata2ts": "^0.24.0",
    "@openui5/ts-types-esm": "1.115.1",
    "@prettier/plugin-xml": "^2.2.0",
    "@types/jquery": "^3.5.16",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "@ui5/cli": "^3.3.1",
    "eslint": "^8.44.0",
    "prettier": "^2.8.4",
    "typescript": "^4.9.5",
    "ui5-middleware-livereload": "^0.8.4",
    "ui5-tooling-modules": "^0.9.12",
    "ui5-tooling-transpile": "^0.7.18"
  },
  "dependencies": {
    "@odata2ts/jquery-odata-client": "^0.3.2",
    "@odata2ts/odata-service": "^0.15.2",
    "mobx": "^6.8.0",
    "ui5-mobx": "^0.1.0"
  }
}
