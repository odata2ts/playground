{
  "compilerOptions": {
    "target": "es2015",
    "module": "es2015",
    "skipLibCheck": true,
    "preserveConstEnums": true,
    "sourceMap": false,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "strictNullChecks": false,
    "strictPropertyInitialization": false,
    "rootDir": "./webapp",
    "outDir": "./dist",
    "baseUrl": "./",
    "moduleResolution": "node",
    "types": ["@openui5/ts-types-esm"],
    "paths": {
      "org/odata2ts/tst/*": ["./webapp/*"]
    }
  },
  "include": ["./webapp/**/*"]
}
